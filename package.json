    "framer-motion": "^11.0.8",
    "react-hook-form": "^7.50.1",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.12.2",
    "date-fns": "^3.3.1",
    "react-calendar": "^4.8.0",
    "react-select": "^5.8.0",
    "react-dropzone": "^14.2.3",
    "socket.io-client": "^4.7.4",
    "axios": "^1.6.7",
    "react-query": "^3.39.3",
